{
	"info": {
		"_postman_id": "00aeffc7-9214-46a8-94cf-13337096449e",
		"name": "spring-thymeleaf",
		"description": "An API developed using Thymeleaf and i18n to generate an PDF\n",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "12072135"
	},
	"item": [
		{
			"name": "v1",
			"item": [
				{
					"name": "Receive the template",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "text/html"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/v1/template/:id?name={{$randomFirstName}}",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"v1",
								"template",
								":id"
							],
							"query": [
								{
									"key": "name",
									"value": "{{$randomFirstName}}",
									"description": "(Required) An parameter passed for code generation"
								},
								{
									"key": "lang",
									"value": "pt",
									"description": "A parameter used for internalization (i18)",
									"disabled": true
								}
							],
							"variable": [
								{
									"key": "id",
									"value": "book_template",
									"description": "(Required) ID to find the template"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "Generate the Base64 PDF",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "application/json"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/v1/pdf/:id?name={{$randomFirstName}}",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"v1",
								"pdf",
								":id"
							],
							"query": [
								{
									"key": "name",
									"value": "{{$randomFirstName}}",
									"description": "(Required) An parameter passed for code generation"
								},
								{
									"key": "lang",
									"value": "en",
									"description": "A parameter used for internalization (i18)",
									"disabled": true
								}
							],
							"variable": [
								{
									"key": "id",
									"value": "book_template",
									"description": "(Required) ID to find the template"
								}
							]
						}
					},
					"response": []
				}
			]
		}
	],
	"variable": [
		{
			"key": "baseUrl",
			"value": "http://localhost:3000",
			"type": "string"
		}
	]
}